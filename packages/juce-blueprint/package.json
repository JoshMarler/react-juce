{
  "name": "juce-blueprint",
  "version": "0.1.0",
  "description": "BlueprintPlugin user interface in React.",
  "main": "./cjs/Blueprint.js",
  "types": "./cjs/Blueprint.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly --outFile ./cjs/Blueprint.d.ts",
    "build:js": "babel esm --out-dir cjs --extensions \".js,.ts\"",
    "init": "node scripts/init.js"
  },
  "author": "Nick Thompson",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/typescript"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/invariant": "^2.2.33",
    "@types/react": "^16.9.41",
    "chalk": "^2.4.2",
    "fs-extra": "^8.1.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "core-js": "2.6.0",
    "invariant": "^2.2.4",
    "react": "^16.6.3",
    "react-reconciler": "^0.17.2"
  }
}
